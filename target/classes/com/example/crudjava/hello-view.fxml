<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="220.0" prefWidth="357.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="com.example.crudjava.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label alignment="CENTER" text="Inscription">
      <font>
         <Font size="29.0" />
      </font>
   </Label>
   <Label text="Nom :">
      <font>
         <Font size="14.0" />
      </font>
   </Label>
   <TextField fx:id="idnom" prefHeight="25.0" prefWidth="157.0" />
   <Label text="Prenom :">
      <font>
         <Font size="14.0" />
      </font>
   </Label>
   <TextField fx:id="idprenom" />
   <Label text="Mail :" />
   <TextField fx:id="idmail" />
   <Label text="Mot de passe :" />
   <TextField fx:id="idmdp" />
   <Label text="Confirmez" />
   <TextField fx:id="idconfirmez" />
   <ToggleButton fx:id="idsauvegarder" mnemonicParsing="false" text="Sauvegarder" />
   <ToggleButton fx:id="idretour" mnemonicParsing="false" prefHeight="25.0" prefWidth="58.0" text="Retour" />
</VBox>
